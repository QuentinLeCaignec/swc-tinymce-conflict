{
  "name": "swc-tinymce-conflict",
  "version": "0.0.1",
  "private": false,
  "author": "Quentin LE CAIGNEC",
  "keywords": [
    "react",
    "next"
  ],
  "scripts": {
    "develop": "next dev --port 3001",
    "build": "next build",
    "start": "next start --port 3001",
    "export": "next export",
    "clean": "rm -rf .cache .tmp .next dist build out coverage",
    "lint": "npm run lint:eslint && npm run lint:style && npm run lint:prettier",
    "lint:eslint": "eslint . --fix --max-warnings=0 --ignore-path .gitignore --ignore-path .eslintignore",
    "lint:style": "stylelint src/**/*.{css,scss,sass} --fix --max-warnings=0 --ignore-path .gitignore",
    "lint:prettier": "prettier . --write --ignore-unknown --ignore-path .gitignore --ignore-path .prettier.ignore"
  },
  "dependencies": {
    "@tinymce/tinymce-react": "4.2.0",
    "next": "13.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tinymce": "6.2.0"
  },
  "devDependencies": {
    "@types/node": "18.11.7",
    "@types/react": "18.0.23",
    "@types/react-dom": "18.0.7",
    "copy-webpack-plugin": "11.0.0",
    "eslint": "8.26.0",
    "eslint-config-next": "13.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-jest-dom": "4.0.2",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-testing-library": "5.9.1",
    "prettier": "2.7.1",
    "sass": "1.55.0",
    "stylelint": "14.14.0",
    "stylelint-config-prettier-scss": "0.0.1",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-config-recommended": "9.0.0",
    "stylelint-config-sass-guidelines": "9.0.1",
    "stylelint-config-standard": "29.0.0",
    "stylelint-config-standard-scss": "6.0.0",
    "stylelint-order": "5.0.0",
    "stylelint-prettier": "2.0.0",
    "typescript": "4.8.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
